{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        /** 定義する前に使われていてもエラーが出ないようにする **/
        "no-use-before-define": 0,
        "react/prop-types": [
            "error",
            {
                "ignore": [
                    /** ナビゲーションのpropのエラー回避 */
                    "navigation"
                ]
            }
        ]
    }
}
